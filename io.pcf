# Clock generated via on board STM32 clone that ack as USB iCELink on iCESugar nano board.
set_io SYS_CLK 	D1

# Tri SPI output pins to MN15439.

# Serial data
set_io S1 	C1
set_io S2	B1
set_io S3	B3

#serial clock
set_io SCK	A3

# Blank and Latch signal
set_io BLK	A1
set_io LAT	A2

# GCP pulse output pin for generating Grayscale image
set_io PWM	B6

# FPGA's slave SPI interface as Framebuffer input.
set_io SSI	E1
set_io SSCK	E2
set_io SCS	F2
set_io EOF	F3